<body>
<h1>{source.label} : {run_date}</h1>

<a href='{source.link}'> {source.label} </a> 

{% for issue_tag, issues in source.grouped_issues.items()%}
	<div>
		<h2> {{ issue_tag }} </h2>
		<ul>
			{% for issue_message in issues %}
			<li> {{issue_message}} </li>
			{% endfor %}
		</ul>
{% endfor %}

</body>